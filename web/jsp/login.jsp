<%@ page contentType="text/html;" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html><html><head><title>Login</title></head><body><form action="/almalaurea/auth" method="post"><br /><br /><div class="container justify-content-center" style="background-color:#ccff66; max-width:400px; min-height:300px; border-radius: 10px 10px 10px 10px;">  <div class="row justify-content-center">    <div class="col-4 text-center">      <h1>Login</h1>    </div>  </div>  <br />  <div class="row justify-content-center"">    <div class="col-8">      <input type="text" class="form-control" name="usr" placeholder="Username">      <br />        <input type="password" class="form-control" name="pwd" placeholder="Password">    </div>  </div>  <br />  <div class="row justify-content-center"">    <div class="col-4 text-center">      <input type="submit" class="btn btn-primary" value="Login">    </div>  </div></div></form><p style="color:red;"><c:out value="${requestScope.msg}" /></p></body></html>